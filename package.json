{
    "name": "primer-react-template",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@babel/core": "^7.25.8",
        "@babel/plugin-syntax-flow": "^7.24.7",
        "@babel/plugin-transform-react-jsx": "^7.24.7",
        "@primer/octicons-react": "^19.11.0",
        "@primer/primitives": "^10.0.0",
        "@primer/react": "^36.27.0",
        "eslint": "^8.57.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-is": "^18.3.1",
        "react-router-dom": "^7.1.3",
        "styled-components": "^6.1.14",
        "web-vitals": "^4.2.4"
    },
    "devDependencies": {
        "eslint-config-react-app": "^7.0.1",
        "react-scripts": "^5.0.1"
    },
    "scripts": {
        "add-yarn": "npm install corepack -g && corepack enable && yarn set version stable",
        "start": "react-scripts start",
        "build": "react-scripts build && rm -rf docs && mv build docs",
        "eject": "react-scripts eject",
        "preview": "react-scripts build && rm -rf docs && mv build docs && http-server docs",
        "deploy": "rm -rf docs && react-scripts build && mv build docs && git add -A && git commit -m \"Build commit\" && git push"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 3 safari version"
        ]
    },
    "packageManager": "yarn@4.6.0"
}
